{
  "$schema": "https://raw.githubusercontent.com/rescript-lang/rescript-compiler/master/docs/docson/build-schema.json",
  "name": "rescript-game",
  "version": "0.0.1",
  "sources": {
    "dir" : "src",
    "subdirs" : true
  },
  "package-specs": {
    "module": "commonjs",
    "in-source": true
  },
  "suffix": ".bs.js",
  "bs-dependencies": [
    "@ambientlight/bs-rx",
    "reason-promise",
    "bs-fetch",
    "bs-webapi",
    "bs-pixi"
  ],
  "warnings": {
    "error" : "+101"
  },
}
